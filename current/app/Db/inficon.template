record(longin, "$(DEV):SYST_STAT")
{
    field(DESC, "read system status")
    field(DTYP, "asynUInt32Digital")
    field(INP,  "@asynMask($(PORT),0,0xFFFFFFFF)SYST_STAT")
    field(SCAN, "$(DSCAN)")
	field(PINI, "YES")
}

